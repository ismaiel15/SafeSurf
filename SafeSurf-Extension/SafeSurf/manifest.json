{
  "manifest_version": 3,
  "name": "SafeSurf",
  "version": "1.0",
  "description": "Protects users from phishing, fake websites, and unsafe password entry.",
  "permissions": [
    "tabs",
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}